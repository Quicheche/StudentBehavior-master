<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentbehavior"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon5"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReportActivity"
            android:label="@string/title_activity_report" >
        </activity>
        <activity
            android:name=".PastReportActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_past_report"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".PastReportListActivity"
            android:label="@string/title_activity_past_report_list" >
        </activity>
        <activity
            android:name=".ItemListActivity"
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name=".ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".ItemListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studentbehavior.ItemListActivity" />
        </activity>
        <activity
            android:name=".ReportList"
            android:label="@string/title_activity_report_list" >
        </activity>
        <activity
            android:name=".ReportListActivity"
            android:label="@string/title_activity_report_list" >
        </activity>
        <activity android:name=".PastReportDetail" />
        <activity
            android:name=".SignUpActivity"
            android:label="@string/sign_up" >
        </activity>
        <activity
            android:name=".GroupReportActivity"
            android:label="@string/title_activity_group_report" >
        </activity>
        <activity
            android:name=".TestActivity"
            android:label="" >
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name=".ViewReportByNameActivity"
            android:label="@string/title_activity_view_report_by_name" >
        </activity>
        <activity
            android:name=".ViewReportByDateActivity"
            android:label="@string/title_activity_view_report_by_date" >
        </activity>
        <activity
            android:name=".LineChartActivity"
            android:label="@string/title_activity_line_chart" >
        </activity>
        <activity
            android:name=".LineChart"
            android:label="@string/title_activity_line_chart" >
        </activity>
        <activity
            android:name=".BarChartActivity"
            android:label="@string/title_activity_bar_chart" >
        </activity>
        <activity
            android:name=".BarChart"
            android:label="@string/title_activity_bar_chart" >
        </activity>
    </application>

</manifest>
